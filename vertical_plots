file.choose()
CROP<-read.csv

names()<-c("x","y","z")
CROP_filter<-CROP[CROP$z>0,]

library(ggplot2)
ggplot(data=CROP_filter,mapping=aes(x=CROP_filter$z))+geom_freqpoly(bins=400)+coord_flip()

OR

ggplot(data=CROP_filter,mapping=aes(x=CROP_filter$z))+geom_freqpoly(bins=400)+coord_flip()+scale_x_continuous(name="height",breaks=c(0,10,20,30), limits=c(0,35))
